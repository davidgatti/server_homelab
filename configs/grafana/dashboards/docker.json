{
  "dashboard": {
    "id": null,
    "title": "HomeLab Services - Docker Monitoring",
    "tags": ["homelab", "docker", "services"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "CPU Usage per Service (%)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=~\"postgres|grafana|prometheus|redis|cadvisor|alertmanager|pgadmin|blackbox-exporter|postgres-exporter|redis-exporter|watchtower|volume-backup|postgres-backup|loki|promtail|speedtest|qdrant|n8n\"}[5m]) * 100",
            "legendFormat": "{{name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 80}
              ]
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 0.2,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "CPU Usage (%)",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": true, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "line"}
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Memory Usage per Service (MB)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "container_memory_usage_bytes{name=~\"postgres|grafana|prometheus|redis|cadvisor|alertmanager|pgadmin|blackbox-exporter|postgres-exporter|redis-exporter|watchtower|volume-backup|postgres-backup|loki|promtail|speedtest|qdrant|n8n\"} / 1024 / 1024",
            "legendFormat": "{{name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 1,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 200},
                {"color": "red", "value": 500}
              ]
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 0.2,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "Memory Usage (MB)",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": true, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "line"}
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Network Traffic - Received (MB/s)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(container_network_receive_bytes_total{name=~\"postgres|grafana|prometheus|redis|cadvisor|alertmanager|pgadmin|blackbox-exporter|postgres-exporter|redis-exporter|watchtower|volume-backup|postgres-backup|loki|promtail|speedtest|qdrant|n8n\"}[5m]) / 1024 / 1024",
            "legendFormat": "{{name}} - RX"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MBs",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": true, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Network Traffic - Transmitted (MB/s)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(container_network_transmit_bytes_total{name=~\"postgres|grafana|prometheus|redis|cadvisor|alertmanager|pgadmin|blackbox-exporter|postgres-exporter|redis-exporter|watchtower|volume-backup|postgres-backup|loki|promtail|speedtest|qdrant|n8n\"}[5m]) / 1024 / 1024",
            "legendFormat": "{{name}} - TX"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MBs",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": true, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Disk I/O - Read (MB/s)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(container_fs_reads_bytes_total{name=~\"postgres|grafana|prometheus|redis|cadvisor|alertmanager|pgadmin|blackbox-exporter|postgres-exporter|redis-exporter|watchtower|volume-backup|postgres-backup|loki|promtail|speedtest|qdrant|n8n\"}[5m]) / 1024 / 1024",
            "legendFormat": "{{name}} - Read"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MBs",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": true, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Disk I/O - Write (MB/s)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(container_fs_writes_bytes_total{name=~\"postgres|grafana|prometheus|redis|cadvisor|alertmanager|pgadmin|blackbox-exporter|postgres-exporter|redis-exporter|watchtower|volume-backup|postgres-backup|loki|promtail|speedtest|qdrant|n8n\"}[5m]) / 1024 / 1024",
            "legendFormat": "{{name}} - Write"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MBs",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": true, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 7,
        "title": "Service Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"cadvisor\"}",
            "legendFormat": "{{instance}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "DOWN", "color": "red"}}, "type": "value"},
              {"options": {"1": {"text": "UP", "color": "green"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "auto"
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 30,
    "style": "dark",
    "version": 0
  },
  "folderId": null,
  "overwrite": false
}
